{
  "title": "track",
  "type": "object",
  "description": "Single track object ",
  "examples": [
    {
      "id": "string",
      "detectionData": {
        "x1": 0,
        "y1": 0,
        "x2": 0,
        "y2": 0,
        "className": "string"
      },
      "velocity": {
        "vx": 0,
        "vy": 0
      },
      "trackLocation": {
        "lat": 0,
        "lon": 0,
        "alt": 0
      },
      "timestamp": 0
    }
  ],
  "properties": {
    "id": {
      "type": "string",
      "description": "The track unique id"
    },
    "detectionData": {
      "type": "object",
      "description": "The coordinates system of the detections will be as followed -\nThe coordinates will be between 0-1 in x and y as refference related to the frame.\nThe top left be (0,0) and bottom right will be (1,1).\nIn case of coordinates smaller then 0 or greater then 1 it will be considered as 0 or 1 accordinly.",
      "required": [
        "x1",
        "y1",
        "x2",
        "y2",
        "className"
      ],
      "properties": {
        "x1": {
          "type": "number",
          "description": "Top left location in frame between 0-1"
        },
        "y1": {
          "type": "number",
          "description": "top-left Y coordinate in frame  between 0-1"
        },
        "x2": {
          "type": "integer",
          "description": "bottom-right X coordinate in frame  between 0-1"
        },
        "y2": {
          "type": "integer",
          "description": "bottom-right Y coordinate in frame  between 0-1"
        },
        "className": {
          "type": "string",
          "description": "The class name",
          "enum": [
            "Person",
            "Vehicle"
          ]
        }
      }
    },
    "velocity": {
      "type": "object",
      "properties": {
        "vx": {
          "type": "number",
          "description": "X vector component"
        },
        "vy": {
          "type": "number",
          "description": "y vector component"
        }
      }
    },
    "trackLocation": {
      "$ref": "./location.json"
    },
    "timestamp": {
      "type": "number",
      "description": "Track updated timestamp"
    }
  },
  "required": [
    "id",
    "detectionData",
    "timestamp"
  ]
}